{
  "files": [
    "index.ts",
    "examples.ts",
    "utils.ts",
    "renders.ts",
    "model.ts",
  ],
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "removeComments": true,
    "noUnusedLocals": true,                      /* Report errors on unused locals. */
    "noUnusedParameters": true,                  /* Report errors on unused parameters. */
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
